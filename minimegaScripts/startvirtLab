#start the vm deployment script

/opt/minimega/bin/minimega -e read /home/bmsalva90/minimegaScripts/virtLab.mm

#attaches the newly created bridge to the physical interface

sysctl -w net.ipv4.ip_forward=1
iptables -t nat -A POSTROUTING -o wlp2s0 -j MASQUERADE
iptables -A INPUT -i nat0 -j ACCEPT
iptables -A INPUT -i wlp2s0 -m state --state ESTABLISHED,RELATED -j ACCEPT
iptables -A OUTPUT -j ACCEPT

